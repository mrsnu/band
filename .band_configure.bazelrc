# Enable TFLite & MNN backend by default
build --define tflite=true
build --define mnn=true

# Band build config for backend setting.
build:tflite --config=tflite
build:tflite --define tflite=true
build:tflite --define tflite=false
build:mnn --config=mnn
build:mnn --define mnn=true
build:mnn --define tflite=false

# Band test config for backend setting.
test:tflite --config=tflite
test:tflite --define tflite=true
test:tflite --define tflite=false
test:mnn --config=mnn
test:mnn --define mnn=true
test:mnn --define tflite=false

# Android build config for Band Engine.
build:android --crosstool_top=//external:android/crosstool
build:android --host_crosstool_top=@bazel_tools//tools/cpp:toolchain
build:android_arm64_tflite --config=android
build:android_arm64_tflite --cpu=arm64-v8a
build:android_arm64_tflite --fat_apk_cpu=arm64-v8a
build:android_arm64_tflite --define tflite=true
build:android_arm64_tflite --define mnn=false
build:android_arm64_mnn --config=android
build:android_arm64_mnn --cpu=arm64-v8a
build:android_arm64_mnn --fat_apk_cpu=arm64-v8a
build:android_arm64_mnn --define mnn=true
build:android_arm64_mnn --define tflite=false
build:android_arm64 --config=android
build:android_arm64 --cpu=arm64-v8a
build:android_arm64 --fat_apk_cpu=arm64-v8a
build:android_arm64 --define mnn=true
build:android_arm64 --define tflite=true

# Android test config for Band Engine.
test:android_arm64_tflite --config=android
test:android_arm64_tflite --cpu=arm64-v8a
test:android_arm64_tflite --fat_apk_cpu=arm64-v8a
test:android_arm64_tflite --define tflite=true
test:android_arm64_tflite --define mnn=false
test:android_arm64_mnn --config=android
test:android_arm64_mnn --cpu=arm64-v8a
test:android_arm64_mnn --fat_apk_cpu=arm64-v8a
test:android_arm64_mnn --define mnn=true
test:android_arm64_mnn --define tflite=false
test:android_arm64 --config=android
test:android_arm64 --cpu=arm64-v8a
test:android_arm64 --fat_apk_cpu=arm64-v8a
test:android_arm64 --define mnn=true
test:android_arm64 --define tflite=true
